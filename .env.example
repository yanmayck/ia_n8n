# Variáveis de Conexão do Banco de Dados (Supabase Transaction Pooler)
POSTGRES_USER="postgres"
POSTGRES_PASSWORD="your_supabase_db_password"
SUPABASE_PROJECT_REF="your_supabase_project_ref"
POSTGRES_DB="postgres"

# Chave secreta para JWT (gere uma string longa e aleatória usando: openssl rand -hex 32)
SECRET_KEY="your_super_secret_jwt_key"

# Senha para acesso à interface de administração (altere para uma senha forte)
ADMIN_PASSWORD="your_strong_admin_password"

# Chaves de API para Serviços Externos
GEMINI_API_KEY="your_gemini_api_key"
GOOGLE_MAPS_API_KEY="your_google_maps_api_key" # Essencial para o cálculo de frete
EVOLUTION_API_KEY="your_evolution_api_key" # Se estiver usando a Evolution API

# Configuração do Supabase (para upload de arquivos)
SUPABASE_URL="https://your_supabase_project_ref.supabase.co"
SUPABASE_KEY="your_supabase_anon_key"
SERVICE_ROLE_KEY="your_supabase_service_role_key"

# Outras configurações
DEBUG=True
